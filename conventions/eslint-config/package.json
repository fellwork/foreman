{
  "name": "@fellwork/eslint-config-foreman",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Conventional ESLint typescript config for the foreman monorepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/fellwork/foreman.git",
    "directory": "conventions/eslint-config"
  },
  "author": "Shane McGuirt <10144278+srmcguirt@users.noreply.github.com>",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": "./bin/eslint.js",
  "files": [
    "lib",
    "*.md"
  ],
  "scripts": {
    "build": "unbuild",
    "commit": "git commit -m \"chore(repo): stage for release\"",
    "discard": "git reset --hard",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs . --fix",
    "prepack": "unbuild",
    "pull": "git pull origin main",
    "push": "git push --follow-tags origin main",
    "stage": "git add .",
    "stash": "git stash",
    "status": "git status",
    "release": "pnpm test && standard-version && pnpm stage && pnpm commit && pnpm push && pnpm publish",
    "test": "pnpm lint"
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/plugin-transform-spread": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@rushstack/eslint-patch": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@typescript-eslint/typescript-estree": "^4.15.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-gitignore": "^0.1.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-yml": "^0.14.0",
    "jsonc-eslint-parser": "^2.1.0",
    "yaml-eslint-parser": "^0.5.0",
    "@babel/preset-env": "~7.16.11"
  },
  "devDependencies": {
    "@fellwork/foreman-typescript": "workspace:*",
    "@jest/types": "^26.1.0",
    "@types/babel__core": "^7.1.12",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^17.0.23",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-jest": "^26.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.5",
    "type-fest": "^0.16.0",
    "unbuild": "latest",
    "standard-version": "~9.3.2"
  },
  "engines": {
    "node": ">=12.20.0",
    "pnpm": ">=6.32.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@6.32.4",
  "keywords": [
    "eslint",
    "config",
    "foreman"
  ]
}
