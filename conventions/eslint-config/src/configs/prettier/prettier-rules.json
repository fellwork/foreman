{
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "printWidth": 100,
        "quoteProps": "as-needed",
        "requirePragma": false,
        "semi": false,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "all",
        "useTabs": false
      }
    ],
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}
