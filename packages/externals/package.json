{
  "name": "@fellwork/foreman-externals",
  "version": "0.0.0",
  "repository": "fellwork/foreman",
  "license": "MIT",
  "author": "Shane McGuirt <shane@fellwork.com>",
  "description": "External dependencies for foreman",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    },
    "./*": {
      "require": "./lib/cjs/*.js",
      "import": "./lib/esm/*.js"
    }
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "/bin",
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && pnpm compile",
    "clean": "rimraf ./lib",
    "compile": "pnpm build:cjs && pnpm build:esm",
    "compile:cjs": "tsc --project tsconfig.cjs.json",
    "compile:esm": "tsc --project tsconfig.esm.json"
  },
  "devDependencies": {
    "@pnpm/exportable-manifest": "^3.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@types/folder-hash": "^4.0.1",
    "@types/fs-extra": "^9.0.13",
    "chalk": "^5.0.1",
    "cosmiconfig": "^7.0.1",
    "date-fns": "^2.28.0",
    "esbuild": "^0.14.28",
    "execa": "^6.1.0",
    "folder-hash": "^4.0.2",
    "fs-extra": "^10.0.1",
    "globby": "^13.1.1",
    "pathe": "^0.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-tsconfig-paths": "^1.1.6",
    "tree-kill": "^1.2.2",
    "tsconfck": "^1.2.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3"
  },
  "packageManager": "pnpm@6.32.3",
  "engines": {
    "node": ">=12.20.0",
    "pnpm": ">=6.32.3"
  },
  "type": "module",
  "tsup": {
    "clean": true,
    "config": false,
    "dts": true,
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "keepNames": true,
    "outDir": "lib",
    "sourcemap": false,
    "splitting": false,
    "target": "esnext",
    "tsconfig": "./tsconfig.json"
  }
}
