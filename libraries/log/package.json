{
  "name": "@fellwork/foreman-log",
  "version": "0.0.0",
  "repository": "fellwork/foreman",
  "license": "MIT",
  "author": "Shane McGuirt <shane@fellwork.com>",
  "description": "",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.cjs",
      "import": "./lib/esm/index.js",
      "default": "./lib/esm/index.js"
    },
    "./*": {
      "require": "./lib/cjs/*.cjs",
      "import": "./lib/esm/*.js"
    }
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "/bin",
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm compile",
    "compile": "tsup src/index.ts --format esm,cjs --sourcemap false --dts --dts-resolve"
  },
  "packageManager": "pnpm@6.32.3",
  "engines": {
    "node": ">=12.20.0",
    "pnpm": ">=6.7.1"
  },
  "type": "module",
  "devDependencies": {
    "esbuild": "^0.14.28",
    "eslint": "latest",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3",
    "@microsoft/api-extractor": "~7.19.5"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "date-fns": "^2.28.0",
    "tslib": "^2.3.1"
  }
}
