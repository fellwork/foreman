{
  "name": "@fellwork/foreman-externals",
  "version": "0.0.0",
  "repository": "fellwork/foreman",
  "license": "MIT",
  "author": "Shane McGuirt <shane@fellwork.com>",
  "description": "External dependencies for foreman",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "require": "./dist/*.cjs",
      "import": "./dist/*.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm compile",
    "compile": "tsup src/index.ts --format esm,cjs --sourcemap false --dts --dts-resolve"
  },
  "dependencies": {
    "@pnpm/exportable-manifest": "^3.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@types/folder-hash": "^4.0.1",
    "@types/fs-extra": "^9.0.13",
    "chalk": "^5.0.1",
    "cosmiconfig": "^7.0.1",
    "date-fns": "^2.28.0",
    "execa": "^6.1.0",
    "folder-hash": "^4.0.2",
    "fs-extra": "^10.0.1",
    "globby": "^13.1.1",
    "pathe": "^0.2.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-tsconfig-paths": "^1.1.6",
    "tree-kill": "^1.2.2",
    "tsconfck": "^1.2.1",
    "tslib": "^2.3.1"
  },
  "packageManager": "pnpm@6.32.3",
  "engines": {
    "node": ">=12.20.0",
    "pnpm": ">=6.32.3"
  },
  "type": "module",
  "devDependencies": {
    "tsup": "^5.12.1",
    "typescript": "^4.6.3"
  }
}
